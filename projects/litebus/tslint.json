{
    "extends": "../../tslint.json",
    "rules": {
        "prefer-const": false,
        "no-shadowed-variable": false,
        "no-unnecessary-initializer": false,
        "no-trailing-whitespace": false,
        "directive-selector": [
            true,
            "attribute",
            "lib",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "lib",
            "kebab-case"
        ]
    }
}
